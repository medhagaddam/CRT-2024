#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>


   int countDigits(int num) {
    int count = 0;
    int temp = num;
    while (temp != 0) {
        temp /= 10;
        count++;
    }
return num % count;
}
int main() {
    int num;
     scanf("%d", &num);
 printf("%d\n", countDigits(num));
 return 0;
}
